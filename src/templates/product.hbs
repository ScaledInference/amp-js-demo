<div class='store'>
  {{#each categories}}
    <div class="category">
      <div class="title">{{title}}</div>
      <div class="products">
      {{#each products}}
        <div class="product">
          <div class="thumbnail">
            <img src="images/products/{{id}}.jpg" alt="image">
          </div>
          <div class="name">{{name}}</div>
          <div class="description">{{description}}</div>
          <div class="price">{{price}}</div>
          <div class="overlay hide">
            <div class="addToCart" data-id={{id}} data-name='{{name}}'' data-description='{{description}}'' data-price={{price}}>Add To Cart</div>
          </div>
        </div>
      {{/each}}
      </div>
    </div>
  {{/each}}
</div>